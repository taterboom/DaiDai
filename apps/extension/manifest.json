{
  "name": "daidai",
  "description": "manage your bookmarks with tags instead of directory",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "48": "images/48.png"
  },
  "action": {
    "default_title": "Read bookmarks",
    "default_popup": "popup/index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["cookies", "bookmarks"],
  "host_permissions": ["https://*.daidai.cyou/"],
  "chrome_url_overrides": {
    "bookmarks": "bookmarks/index.html"
  }
}
