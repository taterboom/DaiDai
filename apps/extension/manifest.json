{
  "name": "daidai",
  "description": "manage your bookmarks with tags instead of directory",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "48": "images/48.png"
  },
  "action": {
    "default_title": "Read bookmarks",
    "default_popup": "popup/index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.daidai.cyou/"],
      "js": ["content_script.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["injectGlobal.js"],
      "matches": ["https://*.daidai.cyou/*"]
    }
  ],
  "permissions": ["cookies"],
  "host_permissions": ["https://*.daidai.cyou/"]
}
